{
  "expo": {
    "name": "Tech Finder",
    "slug": "tech-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/brand/Icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/brand/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F0F0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/brand/Icon.png",
        "backgroundColor": "#F0F0F0"
      }
    },
    "web": {
      "favicon": "./src/assets/brand/Icon.png"
    }
  }
}
